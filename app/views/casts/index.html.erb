<p id="notice"><%= notice %></p>

<h1>Listing Casts</h1>

<table>
  <thead>
    <tr>
      <th>Casts</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @casts.each do |cast| %>
      <tr>

        <td><%= link_to fullname(cast)  , cast %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', root_path %>
